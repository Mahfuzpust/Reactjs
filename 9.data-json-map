App.js---------->


import React from 'react';
import Card from './components/Card';
import Data from './data.json';

let items = [];
items = Data.map((item) => <Card name={item.title} description={item.des}/>)  //map

function App(){
    return <div>
        <h1 className='header'> Todo APP </h1>
        {items} // map call
    </div>
}

export default App;
